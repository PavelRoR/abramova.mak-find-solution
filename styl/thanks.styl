@import '../css/_css/bootstrap.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-family: "OpenSans", sans-serif
	color #000
a
	color: #fefefe
	&:focus
		outline: 0
		text-decoration: none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video_wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 4px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	margin 0 auto
	border-radius: 4px;
	background-color: rgb(255,51,23)
	background-image: linear-gradient( to bottom, rgb(255,56,27) 0%, rgb(236,46,21) 100%);
	width 100%
	max-width: 202px;
	height: 44px;
	line-height: 44px;
	font-size: 15px;
	font-family: "OpenSansBold";
	color: rgb(255, 255, 255);
	text-align: center
	text-decoration: none
	&:focus
		text-decoration: none
		color rgb(255,255,255)
	&:hover
		text-decoration: none
		color rgb(255,255,255)
		background-image: linear-gradient( to top, rgb(255,56,27) 0%, rgb(236,46,21) 100%);
.general_title
	text-align: center
	font-size: 22px;
	font-family: "OpenSansBold", sans-serif;
	line-height: 1.2;
	margin-bottom: 30px
	span
		padding 0 5px 5px
		background-color: rgb(255,240,179)
	.step_span
		text-transform: uppercase
		display: inline-block
		background-color: rgb(255,56,27)
		color #fff
@import 'thanks/main'
@import 'thanks/steps'